{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "getSpecificCaseResponse": {
      "type": "object",
      "properties": {
        "responseCommon": {
          "type": "object",
          "properties": {
            "status": {
              "type": "string",
              "minLength": 1,
              "maxLength": 6,
              "enum": [
                "OK"
              ]
            },
            "processingDate": {
              "$ref": "#/definitions/timestamp"
            },
            "correlationID": {
              "type": "string",
              "minLength": 1,
              "maxLength": 36
            },
            "errorMessage": {
              "type": "string",
              "minLength": 1,
              "maxLength": 512
            },
            "returnParameters": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "paramName": {
                    "type": "string",
                    "enum": [
                      "POSITION"
                    ]
                  },
                  "paramValue": {
                    "type": "string",
                    "enum": [
                      "FAIL"
                    ]
                  }
                },
                "required": [
                  "paramName",
                  "paramValue"
                ],
                "additionalProperties": false
              }
            }
          },
          "required": [
            "status",
            "processingDate"
          ],
          "additionalProperties": false
        },
        "responseDetail": {
          "type": "object",
          "properties": {
            "CDFPayService": {
              "type": "string",
              "minLength": 1,
              "maxLength": 4,
              "enum": [
                "NDRC",
                "SCTY"
              ]
            },
            "CDFPayCaseFound": {
              "type": "boolean"
            },
            "NDRCCase": {
              "type": "object",
              "properties": {
                "CDFPayCaseNumber": {
                  "$ref": "#/definitions/CDFPayCaseNumber"
                },
                "declarationID": {
                  "$ref": "#/definitions/declarationNumber"
                },
                "claimType": {
                  "type": "string",
                  "minLength": 1,
                  "maxLength": 64,
                  "enum": [
                    "C285",
                    "C&E1179"
                  ]
                },
                "caseType": {
                  "type": "string",
                  "minLength": 1,
                  "maxLength": 64,
                  "enum": [
                    "Individual",
                    "Bulk",
                    "CMA",
                    "C18"
                  ]
                },
                "caseStatus": {
                  "$ref": "#/definitions/caseStatus"
                },
                "descOfGoods": {
                  "$ref": "#/definitions/descriptionOfGoods"
                },
                "descOfRejectedGoods": {
                  "$ref": "#/definitions/descriptionOfGoods"
                },
                "declarantEORI": {
                  "$ref": "#/definitions/EORI"
                },
                "importerEORI": {
                  "$ref": "#/definitions/EORI"
                },
                "claimantEORI": {
                  "$ref": "#/definitions/EORI"
                },
                "totalCustomsClaimAmount": {
                  "$ref": "#/definitions/amountType"
                },
                "totalVATClaimAmount": {
                  "$ref": "#/definitions/amountType"
                },
                "totalExciseClaimAmount": {
                  "$ref": "#/definitions/amountType"
                },
                "totalClaimAmount": {
                  "$ref": "#/definitions/amountType"
                },
                "totalCustomsRefundAmount": {
                  "$ref": "#/definitions/amountType"
                },
                "totalVATRefundAmount": {
                  "$ref": "#/definitions/amountType"
                },
                "totalExciseRefundAmount": {
                  "$ref": "#/definitions/amountType"
                },
                "totalRefundAmount": {
                  "$ref": "#/definitions/amountType"
                },
                "totalReimbursmentAmount": {
                  "$ref": "#/definitions/amountType"
                },
                "basisOfClaim": {
                  "type": "string",
                  "minLength": 1,
                  "maxLength": 50,
                  "enum": [
                    "Duplicate Entry",
                    "Duty Suspension",
                    "End Use",
                    "Incorrect Commodity Code",
                    "Incorrect CPC",
                    "Incorrect Value",
                    "Incorrect EORI & Deferment Acc. Num.",
                    "IP",
                    "Miscellaneous",
                    "OPR",
                    "Personal Effects",
                    "Preference",
                    "RGR",
                    "Proof of Return/Refund Given",
                    "Not In Accordance With Contract",
                    "Defective",
                    "Damaged Before Clearance",
                    "Evidence That Goods Have Not Entered The EU",
                    "Incorrect Excise Value",
                    "Incorrect Additional Information Code",
                    "Special Circumstances"
                  ]
                },
                "claimStartDate": {
                  "$ref": "#/definitions/date"
                },
                "claimantName": {
                  "$ref": "#/definitions/claimantName"
                },
                "claimantEmailAddress": {
                  "$ref": "#/definitions/claimantEmailAddress"
                },
                "closedDate": {
                  "$ref": "#/definitions/date"
                },
                "MRNDetails": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/procedureDetail"
                  }
                },
                "entryDetails": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/entryDetail"
                  }
                },
                "reimbursement": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/reimbursementDetail"
                  }
                }
              },
              "required": [
                "CDFPayCaseNumber",
                "claimType",
                "caseType",
                "caseStatus",
                "declarantEORI",
                "importerEORI",
                "claimStartDate"
              ],
              "additionalProperties": false
            },
            "SCTYCase": {
              "type": "object",
              "properties": {
                "CDFPayCaseNumber": {
                  "$ref": "#/definitions/CDFPayCaseNumber"
                },
                "declarationID": {
                  "$ref": "#/definitions/declarationNumber"
                },
                "reasonForSecurity": {
                  "type": "string",
                  "minLength": 1,
                  "maxLength": 3,
                  "enum": [
                    "ACS",
                    "CEP",
                    "CRQ",
                    "CSD",
                    "ENU",
                    "IPR",
                    "MDC",
                    "MDL",
                    "MDP",
                    "MOD",
                    "OPR",
                    "PDD",
                    "RED",
                    "T24",
                    "TA2",
                    "TA3",
                    "TA6"
                  ]
                },
                "procedureCode": {
                  "type": "string",
                  "minLength": 1,
                  "maxLength": 2
                },
                "caseStatus": {
                  "$ref": "#/definitions/caseStatus"
                },
                "goods": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/goodsDetail"
                  }
                },
                "declarantEORI": {
                  "$ref": "#/definitions/EORI"
                },
                "importerEORI": {
                  "$ref": "#/definitions/EORI"
                },
                "claimantEORI": {
                  "$ref": "#/definitions/EORI"
                },
                "totalCustomsClaimAmount": {
                  "$ref": "#/definitions/amountType"
                },
                "totalVATClaimAmount": {
                  "$ref": "#/definitions/amountType"
                },
                "totalClaimAmount": {
                  "$ref": "#/definitions/amountType"
                },
                "totalReimbursementAmount": {
                  "$ref": "#/definitions/amountType"
                },
                "claimStartDate": {
                  "$ref": "#/definitions/date"
                },
                "claimantName": {
                  "$ref": "#/definitions/claimantName"
                },
                "claimantEmailAddress": {
                  "$ref": "#/definitions/claimantEmailAddress"
                },
                "closedDate": {
                  "$ref": "#/definitions/date"
                },
                "reimbursement": {
                  "type": "array",
                  "items": {
                    "$ref": "#/definitions/reimbursementDetail"
                  }
                }
              },
              "required": [
                "CDFPayCaseNumber",
                "reasonForSecurity",
                "procedureCode",
                "caseStatus",
                "declarantEORI",
                "importerEORI",
                "claimStartDate"
              ],
              "additionalProperties": false
            }
          },
          "required": [
            "CDFPayService",
            "CDFPayCaseFound"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "responseCommon"
      ],
      "additionalProperties": false
    }
  },
  "required": [
    "getSpecificCaseResponse"
  ],
  "additionalProperties": false,
  "definitions": {
    "EORI": {
      "type": "string",
      "minLength": 1,
      "maxLength": 17,
      "pattern": "^[A-Z]{2}[0-9A-Z]+$"
    },
    "amountType": {
      "type": "string",
      "pattern": "^-?[0-9]{1,11}[.][0-9]{1,2}$|^-?[0-9]{1,11}$"
    },
    "timestamp": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30,
      "pattern": "^[0-9]{4}\\-[0-9]{2}\\-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}Z"
    },
    "reimbursementDetail": {
      "type": "object",
      "properties": {
        "reimbursementDate": {
          "$ref": "#/definitions/date"
        },
        "reimbursementAmount": {
          "$ref": "#/definitions/amountType"
        },
        "taxType": {
          "type": "string",
          "minLength": 3,
          "maxLength": 3,
          "enum": [
            "A00",
            "A20",
            "A30",
            "A35",
            "A40",
            "A45",
            "B00",
            "A50",
            "A70",
            "A80",
            "A85",
            "A90",
            "A95",
            "B05"
          ]
        },
        "reimbursementMethod": {
          "type": "string",
          "minLength": 1,
          "maxLength": 15,
          "enum": [
            "P",
            "R",
            "C",
            "D",
            "M",
            "G",
            "I"
          ]
        }
      },
      "required": [
        "reimbursementDate",
        "reimbursementAmount",
        "taxType",
        "reimbursementMethod"
      ],
      "additionalProperties": false
    },
    "date": {
      "type": "string",
      "pattern": "^(\\d{8})$"
    },
    "CDFPayCaseNumber": {
      "type": "string",
      "minLength": 1,
      "maxLength": 18
    },
    "claimantName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 512
    },
    "claimantEmailAddress": {
      "type": "string",
      "minLength": 1,
      "maxLength": 241
    },
    "descriptionOfGoods": {
      "type": "string",
      "minLength": 1,
      "maxLength": 500
    },
    "declarationNumber": {
      "type": "string",
      "minLength": 1,
      "maxLength": 18
    },
    "mainDeclarationReference": {
      "type": "boolean"
    },
    "caseStatus": {
      "type": "string",
      "minLength": 1,
      "maxLength": 40,
      "enum": [
        "Open",
        "Open-Analysis",
        "Pending-Approval",
        "Pending-Queried",
        "Resolved-Withdrawn",
        "Rejected-Failed Validation",
        "Resolved-Rejected",
        "Open-Rework",
        "Paused",
        "Resolved-No Reply",
        "RTBH-Sent",
        "Resolved-Refused",
        "Pending Payment Confirmation",
        "Resolved-Approved",
        "Resolved-Partial Refused",
        "Pending Decision Letter",
        "Approved",
        "Analysis-Rework",
        "Rework-Payment Details",
        "Reply To RTBH",
        "Pending-Compliance Recommendation",
        "Pending-Compliance Check Query",
        "Pending-Compliance Check",
        "Pending-Payment",
        "Partial Refund",
        "Resolved-Refund",
        "Pending-Query",
        "Resolved-Manual BTA",
        "Pending-C18",
        "Closed-C18 Raised",
        "RTBH Letter Initiated",
        "Awaiting RTBH Letter Response",
        "Reminder Letter Initiated",
        "Awaiting Reminder Letter Response",
        "Decision Letter Initiated",
        "Partial BTA",
        "Partial BTA/Refund",
        "Resolved-Auto BTA",
        "Resolved-Manual BTA/Refund",
        "Open-Extension Granted",
        "RTBH Sent",
        "Pending-RTBH"
      ]
    },
    "procedureDetail": {
      "type": "object",
      "properties": {
        "MRNNumber": {
          "$ref": "#/definitions/declarationNumber"
        },
        "mainDeclarationReference": {
          "$ref": "#/definitions/mainDeclarationReference"
        }
      },
      "required": [
        "MRNNumber",
        "mainDeclarationReference"
      ],
      "additionalProperties": false
    },
    "goodsDetail": {
      "type": "object",
      "properties": {
        "itemNumber": {
          "type": "string",
          "minLength": 1,
          "maxLength": 2
        },
        "goodsDescription": {
          "$ref": "#/definitions/descriptionOfGoods"
        }
      },
      "required": [
        "itemNumber"
      ],
      "additionalProperties": false
    },
    "entryDetail": {
      "type": "object",
      "properties": {
        "entryNumber": {
          "$ref": "#/definitions/declarationNumber"
        },
        "mainDeclarationReference": {
          "$ref": "#/definitions/mainDeclarationReference"
        }
      },
      "required": [
        "entryNumber",
        "mainDeclarationReference"
      ],
      "additionalProperties": false
    }
  }
}